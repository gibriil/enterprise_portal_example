{
  "realm": "myportal",
  "enabled": true,
  "sslRequired": "NONE",
  "displayName": "My Internal Realm",
  "roles": {
    "realm": [
      { "name": "user", "description": "Standard user role" },
      { "name": "admin", "description": "Administrator role" }
    ],
    "client": {
      "enterprise-portal-client-id": [
        { "name": "portal-user", "description": "Standard client role" },
        { "name": "portal-admin", "description": "Administrator client role" }
      ]
    }
  },
  "groups": [
    {
      "name": "employee",
      "path": "/employee",
      "attributes": {
        "payScale": ["32"]
      },
      "subGroups": [
        {
          "name": "part-time",
          "path": "/employee/part-time",
          "attributes": {
            "payScale": ["33"]
          }
        },
        {
          "name": "full-time",
          "path": "/employee/full-time",
          "attributes": {
            "payScale": ["34"]
          }
        }
      ]
    },
    {
      "name": "sales",
      "path": "/sales",
      "attributes": {
        "payScale": ["31"]
      }
    },
    {
      "name": "csr",
      "path": "/csr",
      "attributes": {
        "payScale": ["35"]
      }
    },
    {
      "name": "team-lead",
      "path": "/team-lead",
      "attributes": {
        "payScale": ["41"]
      }
    }
  ],
  "clients": [
    {
      "clientId": "enterprise-portal-client-id",
      "secret": "enterprise-portal-client-secret",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "redirectUris": ["http://localhost/.oidc", "http://localhost/*", "http://localhost:8500/.oidc", "http://localhost:8500/*"],
      "webOrigins": ["*"],
      "attributes": {
          "access.token.lifespan": "3600",
          "id.token.lifespan": "3600",
          "refresh.token.lifespan": "3600",
          "ssl-required": "NONE"
      },
      "protocolMappers": [
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "realm_roles"
          }
        },
        {
          "name": "client roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "user.clientRoleMapping.clientId": "enterprise-portal-client-id",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "client_roles",
            "jsonType.label": "String"
          }
        },
        {
          "name": "department",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "department",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "department",
            "jsonType.label": "String"
          }
        },
        {
          "name": "entitlementLevel",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "entitlementLevel",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "entitlementLevel",
            "jsonType.label": "String"
          }
        },
        {
          "name": "groups_full",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-group-membership-mapper",
          "config": {
            "full.path": "true",
            "claim.name": "groups_paths",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "groups_short",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-group-membership-mapper",
          "config": {
            "full.path": "false",
            "claim.name": "groups",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "group-entitlementTier",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-group-membership-mapper",
          "consentRequired": false,
          "config": {
            "full.path": "false",
            "add.attributes": "true",
            "attribute.name": "payScale",
            "group.attribute": "payScale",
            "claim.name": "group_entitlementTier",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "multivalued": "true",
            "aggregate.attrs": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "csr",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Charlie",
      "lastName": "Chambers",
      "email": "charlie.chambers@example.com",
      "credentials": [
        { "type": "password", "value": "password123", "temporary": false }
      ],
      "realmRoles": ["user"],
      "clientRoles": {
        "enterprise-portal-client-id": ["portal-user"]
      },
      "groups": ["/employee", "/employee/full-time", "/csr"],
      "attributes": {
        "department": "Inbound",
        "entitlementLevel": "gold"
      }
    },
    {
      "username": "lead",
      "enabled": true,
      "emailVerified": true,
      "firstName": "bobbie",
      "lastName": "Brown",
      "email": "bobbie.brown@example.com",
      "credentials": [
        { "type": "password", "value": "password123", "temporary": false }
      ],
      "realmRoles": ["user"],
      "clientRoles": {
        "enterprise-portal-client-id": ["portal-user"]
      },
      "groups": ["/employee", "/employee/full-time", "/team-lead"],
      "attributes": {
        "department": "leadership",
        "entitlementLevel": "platinum"
      }
    },
    {
      "username": "sales",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Sam",
      "lastName": "Smith",
      "email": "sam.smith@example.com",
      "credentials": [
        { "type": "password", "value": "password123", "temporary": false }
      ],
      "realmRoles": ["user"],
      "clientRoles": {
        "enterprise-portal-client-id": ["portal-user"]
      },
      "groups": ["/employee", "/employee/part-time", "/sales"],
      "attributes": {
        "department": "Cold-Call",
        "entitlementLevel": "gold"
      }
    },
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Gerrit",
      "lastName": "Gopher",
      "email": "go@example.com",
      "credentials": [
        { "type": "password", "value": "password123", "temporary": false }
      ],
      "realmRoles": ["user", "admin"],
      "clientRoles": {
        "enterprise-portal-client-id": ["portal-user", "portal-admin"]
      },
      "groups": ["/employee"],
      "attributes": {
        "department": "C-Suite",
        "entitlementLevel": "Blue Diamond"
      }
    }
  ]
}