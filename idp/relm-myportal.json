{
  "realm": "myportal",
  "enabled": true,
  "sslRequired": "NONE",
  "displayName": "My Internal Realm",
  "roles": {
    "realm": [
      { "name": "user", "description": "Standard user role" },
      { "name": "admin", "description": "Administrator role" }
    ],
    "client": {
      "enterprise-portal-client-id": [
        { "name": "portal-user", "description": "Standard client role" },
        { "name": "portal-admin", "description": "Administrator client role" }
      ]
    }
  },
  "groups": [
    {
      "name": "staff",
      "path": "/staff",
      "attributes": {
        "payScale": ["gold"]
      },
      "subGroups": [
        {
          "name": "executive",
          "path": "/staff/executive",
          "attributes": {
            "payScale": ["platinum"]
          }
        }
      ]
    },
    {
      "name": "c-suit",
      "path": "/c-suit",
      "attributes": {
        "payScale": ["C-Level"]
      }
    }
  ],
  "clients": [
    {
      "clientId": "enterprise-portal-client-id",
      "secret": "enterprise-portal-client-secret",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "redirectUris": ["http://localhost/.oidc"],
      "webOrigins": ["*"],
      "attributes": {
          "access.token.lifespan": "3600",
          "id.token.lifespan": "3600",
          "refresh.token.lifespan": "3600",
          "ssl-required": "NONE"
      },
      "protocolMappers": [
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "realm_roles"
          }
        },
        {
          "name": "client roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "user.clientRoleMapping.clientId": "enterprise-portal-client-id",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "client_roles",
            "jsonType.label": "String"
          }
        },
        {
          "name": "department",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "department",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "department",
            "jsonType.label": "String"
          }
        },
        {
          "name": "entitlementLevel",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-attribute-mapper",
          "consentRequired": false,
          "config": {
            "user.attribute": "entitlementLevel",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "claim.name": "entitlementLevel",
            "jsonType.label": "String"
          }
        },
        {
          "name": "groups_full",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-group-membership-mapper",
          "config": {
            "full.path": "true",
            "claim.name": "groups_paths",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "groups_short",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-group-membership-mapper",
          "config": {
            "full.path": "false",
            "claim.name": "groups",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "group-entitlementTier",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-group-membership-mapper",
          "consentRequired": false,
          "config": {
            "full.path": "false",
            "add.attributes": "true",
            "attribute.name": "payScale",
            "group.attribute": "payScale",
            "claim.name": "group_entitlementTier",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true",
            "multivalued": "true",
            "aggregate.attrs": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "alice",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Alice",
      "lastName": "Anderson",
      "email": "alice@example.com",
      "credentials": [
        { "type": "password", "value": "alice123", "temporary": false }
      ],
      "realmRoles": ["user"],
      "clientRoles": {
        "enterprise-portal-client-id": ["portal-user"]
      },
      "groups": ["/staff"],
      "attributes": {
        "department": "Engineering",
        "entitlementLevel": "gold"
      }
    },
    {
      "username": "bob",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Bob",
      "lastName": "Brown",
      "email": "bob@example.com",
      "credentials": [
        { "type": "password", "value": "bob123", "temporary": false }
      ],
      "realmRoles": ["user", "admin"],
      "clientRoles": {
        "enterprise-portal-client-id": ["portal-user", "portal-admin"]
      },
      "groups": ["/staff/executive", "/c-suit"],
      "attributes": {
        "department": "Finance",
        "entitlementLevel": "platinum"
      }
    }
  ]
}