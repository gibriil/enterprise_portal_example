{
  "realm": "myportal",
  "enabled": true,
  "displayName": "My Internal Realm",
  "roles": {
    "realm": [
      { "name": "user", "description": "Standard user role" },
      { "name": "admin", "description": "Administrator role" }
    ]
  },
  "clients": [
    {
      "clientId": "apache-client",
      "enabled": true,
      "protocol": "openid-connect",
        "publicClient": true,
        "secret": null,
        "redirectUris": ["http://apache:80/redirect_uri", "http://localhost:80/redirect_uri"],
        "webOrigins": ["*"],
        "attributes": {
            "access.token.lifespan": "300",
            "id.token.lifespan": "300",
            "refresh.token.lifespan": "1800"
        }
    }
  ]
}