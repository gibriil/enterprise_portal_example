{
  "realm": "myportal",
  "enabled": true,
  "sslRequired": "NONE",
  "displayName": "My Internal Realm",
  "roles": {
    "realm": [
      { "name": "user", "description": "Standard user role" },
      { "name": "admin", "description": "Administrator role" }
    ]
  },
  "clients": [
    {
      "clientId": "enterprise-portal-client-id",
      "secret": "enterprise-portal-client-secret",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "redirectUris": ["http://localhost/.oidc-redirect"],
      "webOrigins": ["*"],
      "attributes": {
          "access.token.lifespan": "3600",
          "id.token.lifespan": "3600",
          "refresh.token.lifespan": "3600",
          "ssl-required": "NONE"
      }
    }
  ],
  "users": [
    {
      "username": "alice",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Alice",
      "lastName": "Anderson",
      "email": "alice@example.com",
      "credentials": [
        { "type": "password", "value": "alice123", "temporary": false }
      ],
      "realmRoles": ["user"],
      "attributes": {
        "department": "Engineering",
        "entitlementLevel": "gold"
      }
    },
    {
      "username": "bob",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Bob",
      "lastName": "Brown",
      "email": "bob@example.com",
      "credentials": [
        { "type": "password", "value": "bob123", "temporary": false }
      ],
      "realmRoles": ["user", "admin"],
      "attributes": {
        "department": "Finance",
        "entitlementLevel": "platinum"
      }
    }
  ]
}